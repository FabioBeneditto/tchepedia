{
  "name": "tchepedia",
  "description": "Dicionário de termos gaúchos",
  "version": "2.0.0",
  "author": "Rodrigo Silva",
  "homepage": "http://rodrigowaters.github.io/tchepedia/",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "app:build": "ionic cordova build --release",
    "app:emulate": "ionic cordova emulate",
    "app:sign": "jarsigner -verbose -sigalg SHA1withRSA -digestalg SHA1 -keystore tchepedia.keystore platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk alias_name",
    "app:create": "zipalign -v 4 platforms/android/app/build/outputs/apk/release/app-release-unsigned.apk tchepedia.apk",
    "deploy": "ionic build --prod -- --base-href http://rodrigowaters.github.io/tchepedia/ && npx angular-cli-ghpages --dir=www",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "8.2.14",
    "@angular/core": "8.2.14",
    "@angular/forms": "8.2.14",
    "@angular/platform-browser": "8.2.14",
    "@angular/platform-browser-dynamic": "8.2.14",
    "@angular/router": "8.2.14",
    "@ionic-native/core": "5.20.0",
    "@ionic-native/splash-screen": "5.20.0",
    "@ionic-native/status-bar": "5.20.0",
    "@ionic/angular": "4.11.10",
    "@ionic/storage": "2.2.0",
    "angular-cli-ghpages": "0.6.2",
    "cordova-android": "8.1.0",
    "cordova-sqlite-storage": "4.0.0",
    "core-js": "2.6.11",
    "rxjs": "6.5.4",
    "tslib": "1.10.0",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.801.3",
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular-devkit/core": "8.1.3",
    "@angular-devkit/schematics": "8.1.3",
    "@angular/cli": "8.1.3",
    "@angular/compiler": "8.2.14",
    "@angular/compiler-cli": "8.2.14",
    "@angular/language-service": "8.2.14",
    "@ionic/angular-toolkit": "2.1.2",
    "@types/jasmine": "3.3.16",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "8.9.5",
    "codelyzer": "5.2.1",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^4.1.3",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.6",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.5.1",
    "protractor": "5.4.2",
    "ts-node": "7.0.1",
    "tslint": "5.15.0",
    "typescript": "3.4.5"
  },
  "cordova": {
    "plugins": {
      "cordova-sqlite-storage": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-ionic-keyboard": {}
    },
    "platforms": [
      "android"
    ]
  }
}
